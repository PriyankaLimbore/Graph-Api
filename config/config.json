{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "api-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/api/ApiWebPart.js",
          "manifest": "./src/webparts/api/ApiWebPart.manifest.json"
        }
      ]
    },
    "graphapi-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/graphapi/GraphapiWebPart.js",
          "manifest": "./src/webparts/graphapi/GraphapiWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ApiWebPartStrings": "lib/webparts/api/loc/{locale}.js",
    "GraphapiWebPartStrings": "lib/webparts/graphapi/loc/{locale}.js"
  }
}
